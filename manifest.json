{
    "name": "MindCue Extension",
    "version": "1.0.0",
    "description": "This extension is a prototype for a system that helps filter out people's triggers online",
    "manifest_version": 3,
    "author": "Novas",
    "action": {
        "default_popup": "index.html",
        "default_title": "MindCue Extension"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content_script.js"],
          "run_at": "document_end"
        }
      ],
     "permissions": [
       "declarativeContent",
       "storage",
       "tabs",
       "activeTab",
       "windows",
       "scripting"
     ]
}
